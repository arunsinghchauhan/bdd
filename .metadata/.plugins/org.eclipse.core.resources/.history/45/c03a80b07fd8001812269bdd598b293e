package com.basicconcepts;

public class AParent {
	
	 AParent()
	{
		System.out.println("default Const of AParent");
	}
	 AParent(int i)
		{
			 System.out.println("Const of AParent Para");
			
		}
	 public static void main(String[] args) {
		AParent obj1= new AParent();
		 BChild obj2=new BChild(4);
	}

}
class BChild extends AParent
{
	 BChild()
	{
		 System.out.println("default Const of BChild");
		
	}
	 BChild(int i)
		{    //super(i);
			 System.out.println("Const of BChild Para");
			
		}
}