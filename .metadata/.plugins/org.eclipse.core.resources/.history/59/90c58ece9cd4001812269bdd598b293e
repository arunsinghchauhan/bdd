package com.basicconcepts;

public class Poly {

	void function1()
	{
		System.out.println("Task of function 1 in first class");
	}

	public static void main(String[] args) {
		Poly p= new Poly();
		p.function1();
	}
}

class Poly2 extends Poly
{
	void function1()
	{
		System.out.println("Task of function 1 in second class");
	}
}