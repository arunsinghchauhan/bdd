import java.util.ArrayList;

public class ListEx {

	public static void main(String[] args) {

		ArrayList<String> list1 = new ArrayList<String>();
		
		list1.add("Arun");
		list1.add("Anand");
		list1.add("Mit");
		list1.add("Adarsh");
		
		list1.remove("Adarsh");
		
		for(int i=0;i<list1.size();i++)
		{
			System.out.println(list1.get(i));
		}
		

	}

}
